<b>Notifications: </b>
<br>
<% Notification.all.each do |n| %>
	Collection type: <%= n.collectionType %>
	<br>Date: <%= n.date %>
	<br>Owner ID: <%= n.ownerId %>
	<br>Owner Type: <%= n.ownerType %>
	<br>Subscription ID: <%= n.subscriptionId %>
	<br><br>
<% end %>
<br>

<%= link_to "Remove subscription", {:controller => 'fitbit', :action => 'remove_sub'}, :remote => true %>

<% if @fitbit == true %>
	Subscriptions:
	<br><%= @subscriptions %>
<% else %>
 <%= link_to "Add Fitbit Account", {:controller => 'fitbit', :action => 'authorize'} %>
<% end%>